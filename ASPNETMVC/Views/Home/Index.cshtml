@model ASPNETMVC.Models.Employee

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>@ViewBag.Message</p>

<h3>Cars</h3>
<ul>
    @foreach (string car in ViewBag.Cars)
    {
        <li>@car</li>
    }
</ul>

<h3>Fruits</h3>
<ul>
    @foreach (int number in (List<int>)ViewData["Numbers"])
    {
        <li>@number.ToString()</li>
    }
</ul>

<h2>Employee Details</h2>
<table class="table">
    <tbody>
        <tr>
            <td>Id</td>
            <td>@Model.EmployeeId</td>
        </tr>
        <tr>
            <td>Name</td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>City</td>
            <td>@Model.City</td>
        </tr>
    </tbody>
</table>
